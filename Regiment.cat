        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="0f0e-9c96-ed23-49b8" type="min"/>
      </constraints>
      <selectionEntries>
        <selectionEntry id="4034-52fa-102d-77ff" name="Missile weapons" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="360f-cf7f-88d6-699f" type="max"/>
            <constraint field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="b261-7d0b-fa8f-0df9" type="min"/>
          </constraints>
          <profiles>
            <profile id="cfca-cf99-9bcb-4739" name="Missile weapons" hidden="false" typeId="0349-3913-10aa-54e7" typeName="Rules">
              <characteristics>
                <characteristic name="Description" typeId="7556-d5b4-92df-ce01">A unit equipped with missile weapons does not generate a powder smoke token when fired, but decreases the Resilience (Re) their target needs by 1. e.g a 5+ becomes a 4+. Remembering natural 1s always fail.</characteristic>
              </characteristics>
            </profile>
          </profiles>
        </selectionEntry>
        <selectionEntry id="d060-2631-db56-c804" name="Black Powder Weapons" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="abff-a08e-ea1b-fed1" type="max"/>
            <constraint field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="aaf6-7f68-c922-abe3" type="min"/>
          </constraints>
          <profiles>
            <profile id="d139-597d-647a-54ea" name="Black Powder Weapons" hidden="false" typeId="0349-3913-10aa-54e7" typeName="Rules">
              <characteristics>
                <characteristic name="Description" typeId="7556-d5b4-92df-ce01">A unit with black powder weapons generates a powder smoke token every time they make a shooting action.</characteristic>
              </characteristics>
            </profile>
          </profiles>
        </selectionEntry>
        <selectionEntry id="f94e-0732-9b16-a38c" name="Close Combat weapons" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="0d16-cb04-52e4-5835" type="max"/>
            <constraint field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="1602-ec04-db30-579e" type="min"/>
          </constraints>
          <profiles>
            <profile id="515e-93eb-b25b-6f3b" name="Close Combat weapons" hidden="false" typeId="0349-3913-10aa-54e7" typeName="Rules">
              <characteristics>
                <characteristic name="Description" typeId="7556-d5b4-92df-ce01">A unit equipped with close combat weapons cannot make shooting attacks, but increases the Resilience (Re) their target needs by 1 and decrease their own by 1 when fighting in melee.</characteristic>
              </characteristics>
            </profile>
          </profiles>
        </selectionEntry>
      </selectionEntries>
